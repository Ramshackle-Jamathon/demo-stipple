{
	root: true,
	extends: ["eslint:recommended"],
	env: {
		es6: true,
		node: true,
		browser: true
	},
	parserOptions: {
		ecmaVersion: 6
	},
	parser: "babel-eslint",
	rules: {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"brace-style": [2, "stroustrup", { "allowSingleLine": false }],
		"no-catch-shadow": 2,
		"no-console": 2,
		"no-eq-null": 2,
		"no-floating-decimal": 2,
		"no-inline-comments": 0,
		"no-param-reassign": 0,
		"no-self-compare": 2,
		"no-shadow": 0,
		"no-throw-literal": 2,
		"no-undefined": 0,
		"no-unused-vars": 2,
		"no-void": 2,
		"quote-props": [2, "as-needed", { "keywords": true, "unnecessary": false }],
		"quotes": [1, "double"],
		"semi": 2,
		"keyword-spacing": 2,
		"wrap-iife": [2, "outside"],
		"dot-notation": 0,
		"require-yield": 0,
		"react/prop-types": 0
	}
}